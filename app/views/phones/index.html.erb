<h1>Phones</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>Number</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @phones.each do |phone| %>
      <tr>
        <td><%= phone.id %></td>
        <td><%= link_to phone.number, phone_path(phone) %></td>
        <td><%= phone.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_phone_path(phone), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', phone_path(phone), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_phone_path, :class => 'btn btn-primary' %>
